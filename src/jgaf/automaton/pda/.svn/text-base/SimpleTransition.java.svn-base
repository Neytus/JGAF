package jgaf.automaton.pda;

/**
 *
 * @author hanis
 */
public class SimpleTransition {
    
    private Ternary ternary;
    private Pair pair;
        
    public SimpleTransition(Ternary ternary, Pair pair) {
        this.setTernary(ternary);
        this.setPair(pair);
    }

    public Ternary getTernary() {
        return ternary;
    }

    public void setTernary(Ternary ternary) {
        this.ternary = ternary;
    }

    public Pair getPair() {
        return pair;
    }

    public void setPair(Pair pair) {
        this.pair = pair;
    }
    
    public String toString() {
        return ternary + " -> " + pair;
    }
    
}
