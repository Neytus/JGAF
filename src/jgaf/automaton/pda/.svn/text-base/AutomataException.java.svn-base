package jgaf.automaton.pda;

/**
 *
 * @author hanis
 */
public class AutomataException extends Exception {
    
    public AutomataException() {
        super();
    }
            
    public AutomataException(String message) {
        super(message);
    }
    
    public AutomataException(String message, Throwable e) {
        super(message,e);
    }
}