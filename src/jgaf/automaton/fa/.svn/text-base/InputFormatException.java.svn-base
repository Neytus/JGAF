package jgaf.automaton.fa;

/**
 *
 * @author hanis
 */
public class InputFormatException extends Exception {

    /**
     * Creates a new instance of <code>InputFormatException</code> without detail message.
     */
    public InputFormatException() {
    }


    /**
     * Constructs an instance of <code>InputFormatException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public InputFormatException(String msg) {
        super(msg);
    }
}
