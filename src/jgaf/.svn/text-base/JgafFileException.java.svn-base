package jgaf;

/**
 *
 * @author hanis
 */
public class JgafFileException extends Exception {

    /**
     * Creates a new instance of <code>JgafFileException</code> without detail message.
     */
    public JgafFileException() {
    }


    /**
     * Constructs an instance of <code>JgafFileException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public JgafFileException(String msg) {
        super(msg);
    }
}
