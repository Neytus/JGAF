package jgaf.grammar;

import java.util.Comparator;

/**
 *
 * @author hanis
 */
public class ProductionRuleDescendingComparator implements Comparator<ProductionRule>{

    public int compare(ProductionRule o1, ProductionRule o2) {
        return o2.compareTo(o1);
    }
}
