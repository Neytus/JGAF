package jgaf.editor;

/**
 *
 * @author hanis
 */
public class NoSuchEditorException extends Exception {

    /**
     * Creates a new instance of <code>NoSuchEditorException</code> without detail message.
     */
    public NoSuchEditorException() {
    }


    /**
     * Constructs an instance of <code>NoSuchEditorException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public NoSuchEditorException(String msg) {
        super(msg);
    }
}
