package jgaf.exporter;

/**
 *
 * @author hanis
 */
public class TXTWriterException extends Exception {

    /**
     * Creates a new instance of <code>TXTWriterException</code> without detail message.
     */
    public TXTWriterException() {
    }


    /**
     * Constructs an instance of <code>TXTWriterException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public TXTWriterException(String msg) {
        super(msg);
    }
}
